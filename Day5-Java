public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        
        for (int i = 1 ; i <=10; i++) {
            
            int result = n * i;
          if( n >= 2 && n <= 20) {
                
                System.out.println(n + " x " + i + " = " + result);
            }
        }
    }
}
